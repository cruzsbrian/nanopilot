#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'vcstool==0.1.36','console_scripts','vcs-pull'
__requires__ = 'vcstool==0.1.36'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('vcstool==0.1.36', 'console_scripts', 'vcs-pull')()
    )
