#! /usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'colcon-core==0.3.14','console_scripts','colcon'
__requires__ = 'colcon-core==0.3.14'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('colcon-core==0.3.14', 'console_scripts', 'colcon')()
    )
